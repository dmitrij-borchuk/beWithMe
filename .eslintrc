{
  "extends": "airbnb",
  "globals": {
    "describe": true,
    "it": true,
    "xit": true,
    "expect": true,
    "jest": true,
    "afterEach": true,
    "Audio": true,
    "window": true
  },
  "rules": {
    "function-paren-newline": [
      "error",
      "consistent"
    ],
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": [
          "Link"
        ],
        "specialLink": [
          "to"
        ],
        "aspects": [
          "noHref",
          "invalidHref",
          "preferButton"
        ]
      }
    ]
  }
}